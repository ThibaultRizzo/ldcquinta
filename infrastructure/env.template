# AWS Configuration for LDC Quinta
# Copy this file to .env and fill in your values

# S3 Bucket name (should match your domain)
AWS_S3_BUCKET=student.luzqr.com

# AWS Region (use us-east-1 for certificate)
AWS_REGION=us-east-1

# CloudFront Distribution ID (get this after creating distribution)
CLOUDFRONT_DISTRIBUTION_ID=E1ABCDEFGHIJK

# Domain configuration
DOMAIN_NAME=student.luzqr.com
ROOT_DOMAIN=luzqr.com

# AWS Credentials (DO NOT commit these!)
# Set these as environment variables or GitHub secrets
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key

